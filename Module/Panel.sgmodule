#!name=信息面板
#!desc=展示网络状态、刷新 DNS、节点解锁、订阅信息等

[Script]
net-info-panel = type=generic,timeout=5,script-path=https://raw.githubusercontent.com/Peng-YM/QuanX/master/Tools/Panels/NetInfo/net-info-panel.js
flushDNS = type=generic,timeout=10,script-path=https://raw.githubusercontent.com/zZPiglet/Task/master/asset/flushDNS.js,argument=title=DNS FLush&icon=arrow.clockwise&color=#3d3d5b
nf_check = type=generic,timeout=10,script-path=https://gist.githubusercontent.com/Hyseen/b06e911a41036ebc36acf04ddebe7b9a/raw/nf_check.js
ytb_check = type=generic,timeout=10,script-path=https://gist.githubusercontent.com/Hyseen/5ae36a6a5cb5690b1f2bff4aa19c766f/raw/youtube_premium_check.js
Sub_info = type=generic,timeout=10,script-path=https://raw.githubusercontent.com/mieqq/mieqq/master/sub_info_panel.js,script-update-interval=0,argument=url=https%3A%2F%2Fsub.store%2Fdownload%2FAmy&reset_day=28&title=AmyInfo&color=#007aff

[Panel]
net-info-panel = script-name=net-info-panel, title="网络状态", content="请刷新", style=info
flushDNS = script-name=flushDNS, icon=arrow.clockwise, update-interval=600
nf_check = script-name=nf_check, title="Netflix 解锁检测", content="请刷新", update-interval=600
ytb_check = script-name=ytb_check, title="YouTube Premium 解锁检测", update-interval=600
Sub_info = script-name=Sub_info,update-interval=600
